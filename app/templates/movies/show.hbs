
<div class="app-bar-shadow small-screen-only medium-screen-only"></div>
<div class="app-panel movie-reader">
  <div class="movie-reader-header">
    {{movie-card movie=model}}
    {{#tabbed-links}}
      {{link-to "Torrents"  "movies.show.torrents" (query-params highlight=null) replace=true}}
      {{link-to "Comments" "movies.show.comments" replace=true}}
    {{/tabbed-links}}
  </div>

  <div class="movie-reader-content internal">
    {{outlet}}
  </div>
</div>
